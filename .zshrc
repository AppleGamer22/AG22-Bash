function updatePod() {
	read "input?Update Cocoapods? ([y/n], ^C to exit): "
	case $input in
	[yY])
				echo "$ sudo gem install cocoapods"
				sudo gem install cocoapods
		;;
	[nN]);;
		*)
			updatePod
		;;
	esac
}

function updateNode() {
	npm completion >> ~/.zshrc
	read "input?Update Node.js & NPM packages? ([y/n], ^C to exit): "
	case $input in
		[yY])
				echo "$ sudo n lts"
				sudo n lts
				echo "$ sudo n prune"
				sudo n prune
				echo "$ sudo npm update -g"
				sudo npm update -g
		;;
	[nN]);;
		*)
			updateNode
		;;
	esac
}

function reclaim() {
	sudo chown -R $USER:$GROUP ~/.npm
	sudo chown -R $USER "$1"
}

function hide() {
	sudo chflags hidden "$1"
	killall Finder
}

function show() {
	sudo chflags nohidden "$1"
	killall Finder
}

function encrypt() {
	openssl enc -aes-256-cbc -e -in "$1" -out "$2"	
}

function decrypt() {
	openssl enc -aes-256-cbc -d -in "$1" -out "$2"
}
